{% extends '::base.html.twig' %}

{% block body -%}
    <h1>UserInfo list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fname</th>
                <th>Lname</th>
                <th>Email</th>
                <th>State</th>
                <th>Country</th>
                <th>Zipcode</th>
                <th>Mobile</th>
                <th>Usertype</th>
                <th>Createddate</th>
                <th>Updateddate</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('userinfo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.fName }}</td>
                <td>{{ entity.lName }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.state }}</td>
                <td>{{ entity.country }}</td>
                <td>{{ entity.zipCode }}</td>
                <td>{{ entity.mobile }}</td>
                <td>{{ entity.userType }}</td>
                <td>{% if entity.createdDate %}{{ entity.createdDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.updatedDate %}{{ entity.updatedDate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('userinfo_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('userinfo_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('userinfo_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
