{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BidsInfo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Busername</th>
                <td>{{ entity.bUserName }}</td>
            </tr>
            <tr>
                <th>Bemail</th>
                <td>{{ entity.bEmail }}</td>
            </tr>
            <tr>
                <th>Bproductname</th>
                <td>{{ entity.bProductName }}</td>
            </tr>
            <tr>
                <th>Bamount</th>
                <td>{{ entity.bAmount }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updateddate</th>
                <td>{{ entity.updatedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('bidsinfo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('bidsinfo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
